server:
  cors: http://localhost
  port: 8080
  limit: 1h
agent:
  enable: false
  flow:
    discover: false
  item:
    concurrency: 3
    max: 3000
  discover:
    level: 0 #0-no discover;1-discover by id;2-discover by id and domain
datasource:
  driver: mysql
  host: localhost
  port: 3306
  database: mouban
  username: root
  password: 123456
  charset: utf8mb4
  loc: Asia/Shanghai
http:
  timeout: 10
  retry_max: 20
  interval:
    user: 4
    item: 4
    discover: 4